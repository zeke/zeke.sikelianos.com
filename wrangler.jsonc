{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "website",
  "main": "src/worker.js",
  "compatibility_date": "2026-02-05",
  "workers_dev": true,
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS",
    "run_worker_first": true,
    "not_found_handling": "404-page",
    "html_handling": "auto-trailing-slash"
  }
}
